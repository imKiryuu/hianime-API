# CORS origins for production (comma-separated)
ORIGIN=https://yourdomain.vercel.app,https://yourdomain.com

# Upstash Redis configuration (optional, for caching)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Base URL for the API (will be set by Vercel automatically)
BASE_URL=

# Rate limiting configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_LIMIT=100

# Node environment
NODE_ENV=production